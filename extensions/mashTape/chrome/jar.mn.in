mashTape.jar:
% content mashtape %content/ contentaccessible=yes
% resource mashtape modules/
% locale mashtape en-US %locale/en-US/
#% locale mashtape bn-IN %locale/bn-IN/
% locale mashtape ca %locale/ca/
#% locale mashtape cs %locale/cs/
#% locale mashtape da %locale/da/
% locale mashtape de %locale/de/
% locale mashtape el %locale/el/
#% locale mashtape en-GB %locale/en-GB/
#% locale mashtape es-AR %locale/es-AR/
% locale mashtape es-ES %locale/es-ES/
% locale mashtape fr %locale/fr/
#% locale mashtape he %locale/he/
% locale mashtape hu %locale/hu/
% locale mashtape it %locale/it/
% locale mashtape ja %locale/ja/
#% locale mashtape kw %locale/kw/
% locale mashtape nl %locale/nl/
#% locale mashtape pl %locale/pl/
#% locale mashtape pt-BR %locale/pt-BR/
% locale mashtape pt-PT %locale/pt-PT/
% locale mashtape ru %locale/ru/
% locale mashtape sl %locale/sl/
#% locale mashtape sr %locale/sr/
#% locale mashtape sv-SE %locale/sv-SE/
#% locale mashtape th %locale/th/
#% locale mashtape zh-CN %locale/zh-CN/
% locale mashtape zh-TW %locale/zh-TW/
% skin mashtape classic/1.0 %skin/
% overlay chrome://browser/content/preferences/preferences.xul chrome://mashtape/content/prefsOverlay.xul
% style chrome://songbird/content/xul/layoutBaseOverlay.xul chrome://mashtape/skin/mainwin.css
% override chrome://songbird-web/content/html.css chrome://songbird/skin/html.css
% override chrome://songbird-web/skin/base-elements/icon-loading-large.png chrome://songbird/skin/base-elements/icon-loading-large.png
% style chrome://mashtape/content/mashTape.xul chrome://mashtape/skin/styles-win.css os=WinNT
% content songbird-web foo/ contentaccessible=yes
* content/mashTape.js (content/mashTape.js)
@include_all_to
