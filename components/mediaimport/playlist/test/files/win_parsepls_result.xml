<?xml version="1.0"?>
<item-tests>
  <item-test
    property="#contentURL"
    value="file:///h:/media/music/aus/2006-06-25%20(abstract,%20techno,%20punk).mp3"
  >
    <assert-property-value
      name="#contentURL"
      value="file:///h:/media/music/aus/2006-06-25%20(abstract,%20techno,%20punk).mp3"
    />
    <assert-property-value
      name="#trackName"
      value="2006-06-25 (Abstract, Techno, Punk)"
    />
    <assert-property-value
      name="#duration"
      value="3155000000"
    />
  </item-test>
  <item-test
    property="#contentURL"
    value="file:///c:/foo1.mp3"
  >
    <assert-property-value
      name="#contentURL"
      value="file:///c:/foo1.mp3"
    />
    <assert-property-value
      name="#trackName"
      value="==="
    />
  </item-test>
  <item-test
    property="#contentURL"
    value="http://216.66.69.246:8072/"
  >
    <assert-property-value
      name="#contentURL"
      value="http://216.66.69.246:8072/"
    />
    <assert-property-value
      name="#trackName"
      value="(#1 - 553/1000) 1.FM - Channel X"
    />
  </item-test>
</item-tests>

